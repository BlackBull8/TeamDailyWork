<Application x:Class="TeamDailyWork.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TeamDailyWork"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/CheckBoxStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/RadioButtonStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/TextBoxStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/ButtonStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/CalendarStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/PreNextDayButtonStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/ColorListBoxStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/ComboBoxStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
